@page
@model Hillerød_Sejlklub.Pages.Users.AllUsersListModel
@{
}
<h1>Medlemmer</h1>
<table class="table table-bordered table-hover table-striped">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Navn
            </th>
            <th>
                Email
            </th>
            <th>
                Telefon
            </th>
        </tr>
    </thead>
    <tbody>
        @if (Model.Users != null)
        {
            foreach (var item in Model.Users)
            {
                <tr>
                    <td>
                        @item.Id
                    </td>
                    <td>
                        @item.Name
                    </td>
                    <td>
                        @item.Email
                    </td>
                    <td>
                        @item.Phone
                    </td>
                </tr>
            }
        }
    </tbody>
</table>